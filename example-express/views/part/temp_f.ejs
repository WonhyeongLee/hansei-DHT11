<table id="data-table" border="1">
    <tr>
        <th>센서ID</th>
        <th>화씨 값</th>
        <th>날짜 및 시간</th>
    </tr>
    <% data.forEach(function(row, index){ %>
    <tr>
        <td><%= row.sensor %></td>
        <td><%= row.temp_f %></td>
        <td><%= row.collect_time %></td>
    </tr>
    <% }); %>
</table>

<input type="button" value="뒤로가기" onClick="location.href='http://127.0.0.1:3000/index.ejs'">